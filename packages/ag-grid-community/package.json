{
  "name": "ag-grid-community",
  "version": "21.2.2",
  "description": "Advanced Data Grid / Data Table supporting Javascript / React / AngularJS / Web Components",
  "main": "./main.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json src/ts/**/*.ts",
    "lint-fix": "tslint -p tsconfig.json -c tslint-fix.json --fix src/ts/**/*.ts",
    "watchWebpack": "webpack --watch",
    "precheck": "tsc -p tsconfig-precheck.json",
    "build": "npm run precheck && gulp release",
    "test": "exit 0",
    "test:e2e": "npm run webdriver-start && npm run ci || npm run webdriver-kill",
    "e2e": "protractor protractor.conf.js",
    "webdriver-start": "npm run update-webdriver && webdriver-manager start --standalone --detach",
    "webdriver-stop": "webdriver-manager shutdown",
    "webdriver-kill": "kill $(ps -ef | grep webdriver | grep -v grep | awk '{ print $2 }')",
    "update-webdriver": "webdriver-manager update --gecko false",
    "start-server": "http-server .",
    "ci": "start-server-and-test start-server http://localhost:8080 e2e"
  },
  "typings": "./main.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "keywords": [
    "web-components",
    "grid",
    "data",
    "table",
    "angular",
    "angular-component",
    "react",
    "react-component",
    "angularjs",
    "reactjs"
  ],
  "jspm": {
    "main": "jspm-main"
  },
  "author": "Niall Crosby <niall.crosby@ag-grid.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "homepage": "http://www.ag-grid.com/",
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "cache-loader": "^1.2.5",
    "css-loader": "^1.0.1",
    "del": "^3.0.0",
    "file-loader": "3.0.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-filter": "^5.1.0",
    "gulp-flatmap": "1.0.2",
    "gulp-header": "^2.0.5",
    "gulp-jasmine": "^4.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-stylus": "^2.7.0",
    "gulp-tsd": "^0.1.1",
    "gulp-typescript": "^5.0.0-alpha.3",
    "gulp-uglify": "^3.0.1",
    "http-server": "^0.11.1",
    "image-webpack-loader": "^4.5.0",
    "jasmine-core": "^3.3.0",
    "json-stable-stringify": "^1.0.1",
    "merge2": "^1.2.3",
    "mini-css-extract-plugin": "^0.4.5",
    "nib": "^1.1.2",
    "node-sass": "^4.10.0",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "postcss-svgo": "^4.0.1",
    "protractor": "^5.4.2",
    "sass-loader": "^7.1.0",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "tslint": "^5.11.0",
    "typescript": "~3.4.1",
    "url-loader": "^1.1.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-named": "^1.1.0",
    "webpack": "^4.26.1",
    "webpack-stream": "^5.1.1"
  }
}