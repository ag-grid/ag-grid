// import "ag-grid-community/styles/ag-theme-alpine.css"; // import Alpine theme, not used here

const VueExample = {
 template: `
       <div style="height: 100%">
           <div style="height: 100%; box-sizing: border-box;">
               <ag-grid-vue
               // ...
               :class="themeClass"></ag-grid-vue>
           </div>
       </div>
   `,
// ...
 data: function () {
   return {
     // ...
