{
  "name": "ag-grid-generate-code-reference-files",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "plugins/ag-grid-generate-code-reference-files/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "dependsOn": ["copySrcFilesForGeneration"],
      "inputs": ["{projectRoot}/src/**"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{projectRoot}/dist",
        "main": "{projectRoot}/src/index.ts",
        "tsConfig": "{projectRoot}/tsconfig.lib.json",
        "assets": [
          "plugins/ag-grid-generate-code-reference-files/*.md",
          {
            "input": "./plugins/ag-grid-generate-code-reference-files/src",
            "glob": "**/!(*.ts|*.snap)",
            "output": "./src"
          },
          {
            "input": "./plugins/ag-grid-generate-code-reference-files/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./plugins/ag-grid-generate-code-reference-files",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./plugins/ag-grid-generate-code-reference-files",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "copySrcFilesForGeneration": {
      "executor": "nx:run-script",
      "options": {
        "script": "copySrcFilesForGeneration"
      },
      "cache": false,
      "inputs": ["{workspaceRoot}/packages/ag-grid-community/src/eventTypes.ts"],
      "outputs": [
        "{workspaceRoot}/plugins/ag-grid-generate-example-files/src/executors/generate/generator/_copiedFromCore/eventTypes.ts"
      ]
    },
    "lint": {
      "command": "eslint",
      "options": {
        "cwd": "{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "plugins/ag-grid-generate-code-reference-files/jest.config.ts"
      },
      "configurations": {
        "update": {
          "updateSnapshot": true
        }
      }
    }
  },
  "tags": []
}
