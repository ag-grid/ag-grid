{
  // See http://go.microsoft.com/fwlink/?LinkId=827846
  // for the documentation about the extensions.json format
  "recommendations": [
    // mono repo import path fixer
    "q.typescript-mono-repo-import-helper",
    // git helper
    "eamodio.gitlens",
    // code spelling
    "streetsidesoftware.code-spell-checker",
    // sass helper
    "syler.sass-indented",
    // shows the cost of each import
    "wix.vscode-import-cost",
    // shows the cost of CSS properties
    "kisstkondoros.csstriggers",
    // es6 string HTML highlight
    "tobermory.es6-string-html",
    // prettier
    "esbenp.prettier-vscode",
    // markdoc
    "stripe.markdoc-language-support",
    // vitest test runner, used for behavioural tests
    "vitest.explorer"
  ]
}
