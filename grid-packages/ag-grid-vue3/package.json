{
  "name": "ag-grid-vue3",
  "description": "AG Grid Vue 3 Component",
  "version": "27.3.0",
  "author": "Sean Landsman <sean@thelandsmans.com>",
  "license": "MIT",
  "main": "./lib/AgGridVue.js",
  "typings": "./lib/AgGridVue.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "homepage": "https://www.ag-grid.com/",
  "keywords": [
    "grid",
    "data",
    "table",
    "vue",
    "vuejs"
  ],
  "scripts": {
    "clean": "rimraf lib dist .hash",
    "build": "npx gulp copy-from-module-source && vue-cli-service build --target lib src/AgGridVue.ts && npx tsc && rm ./dist/demo.html && npm run hash",
    "build-prod": "npm run build",
    "hash": "sh ../../scripts/hashDirectory.sh > .hash",
    "test:unit": "vue-cli-service test:unit",
    "test:e2ex": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "ag-grid-community": "~27.3.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "typescript": "~4.1.5",
    "vue-jest": "^5.0.0-0",
    "gulp": "^4.0.0",
    "gulp-typescript": "^5.0.0",
    "gulp-replace": "^1.0.0",
    "rimraf": "3.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}