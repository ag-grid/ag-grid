        "columnPivotChanged": {
            "description": "The pivot value has changed.",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
            }
        },
        "columnValueChanged": {
            "description": "The 'value' value has changed.",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
