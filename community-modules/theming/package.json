{
    "name": "@ag-grid-enterprise/theming",
    "version": "31.2.0",
    "private": true,
    "description": "Advanced Data Grid / Data Table supporting Javascript / Typescript / React / Angular / Vue",
    "scripts": {
        "codegen:prod": "tsx scripts/codegen.ts",
        "codegen:dev": "tsx scripts/codegen.ts --dev --no-clean",
        "codegen:watch": "chokidar 'src/**/*' --ignore '**/GENERATED-*' --command 'npm run codegen:dev'"
    },
    "dependencies": {
        "tslib": "^2.3.0"
    },
    "main": "./dist/package/main.cjs.js",
    "types": "./dist/types/src/main.d.ts",
    "module": "./dist/package/main.esm.mjs",
    "exports": {
        "import": "./dist/package/main.esm.mjs",
        "require": "./dist/package/main.cjs.js",
        "types": "./dist/types/src/main.d.ts",
        "default": "./dist/package/main.cjs.js"
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/postcss-import": "^14.0.3",
        "@types/postcss-url": "^10.0.4",
        "autoprefixer": "^10.4.19",
        "chokidar-cli": "^3.0.0",
        "cssnano": "^6.1.0",
        "glob": "^10.3.10",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.7.0",
        "postcss-import": "^16.0.1",
        "postcss-nesting": "^12.0.1",
        "postcss-rtlcss": "^4.0.9",
        "postcss-url": "^10.1.3",
        "sass": "^1.71.1",
        "tsx": "^4.7.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ag-grid/ag-grid.git"
    },
    "keywords": [
        "web-components",
        "grid",
        "data",
        "table",
        "angular",
        "angular-component",
        "react",
        "react-component",
        "reactjs",
        "vue",
        "vuejs"
    ],
    "author": "Sean Landsman <sean@thelandsmans.com>",
    "license": "Commercial",
    "bugs": {
        "url": "https://github.com/ag-grid/ag-grid/issues"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie >= 0",
        "not ie_mob >= 0",
        "not blackberry > 0"
    ],
    "homepage": "https://www.ag-grid.com/",
    "publishConfig": {
        "access": "public"
    }
}
