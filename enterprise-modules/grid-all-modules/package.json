{
  "name": "@ag-enterprise/grid-all-modules",
  "version": "22.0.0",
  "description": "Advanced Data Grid / Data Table supporting Javascript / React / AngularJS / Web Components",
  "main": "./dist/cjs/main.js",
  "module": "./dist/es6/main.js",
  "scripts": {
    "build-cjs": "tsc -p tsconfig.es5.json",
    "build-es6": "tsc -p tsconfig.es6.json",
    "build-docs": "tsc -p tsconfig.docs.json",
    "build": "npm run precheck && npm run lint && npm run build-cjs && npm run build-es6",
    "package": "gulp copy-grid-core-styles && npm run webpack-updater && npx gulp webpack-all-no-clean && npm run rollup",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --no-cache -runInBand",
    "testWithCoverage": "jest --coverage",
    "watchWebpack": "webpack --watch",
    "precheck": "tsc -p tsconfig-precheck.json",
    "webpack-updater": "node webpackUpdater.js",
    "rollup": "node rollup/build.js",
    "lint": "tsc -p tsconfig-lint.json && tslint --project tsconfig.es5.json",
    "test:e2e": "npm run webdriver-start && npm run ci || npm run webdriver-kill",
    "e2e": "protractor protractor.conf.js",
    "e2e:debug": " node --inspect-brk ./node_modules/.bin/protractor protractor.conf.js",
    "webdriver-start": "npm run update-webdriver && webdriver-manager start --standalone --detach",
    "webdriver-stop": "webdriver-manager shutdown",
    "webdriver-kill": "kill $(ps -ef | grep webdriver | grep -v grep | awk '{ print $2 }')",
    "update-webdriver": "webdriver-manager update --gecko false",
    "start-server": "http-server .",
    "ci": "start-server-and-test start-server http://localhost:8080 e2e",
    "validate-es5": "eslint dist/lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "keywords": [
    "web-components",
    "grid",
    "data",
    "table",
    "angular",
    "angular-component",
    "react",
    "react-component",
    "angularjs",
    "reactjs"
  ],
  "author": "Sean Landsman <sean@thelandsmans.com>",
  "license": "Commercial",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 11",
    "not blackberry > 0"
  ],
  "homepage": "http://www.ag-grid.com/",
  "dependencies": {
    "@ag-community/grid-all-modules": "^22.0.0",
    "@ag-community/grid-client-side-row-model": "^22.0.0",
    "@ag-community/grid-core": "^22.0.0",
    "@ag-community/grid-csv-export": "^22.0.0",
    "@ag-community/grid-infinite-row-model": "^22.0.0",
    "@ag-enterprise/grid-charts": "^22.0.0",
    "@ag-enterprise/grid-clipboard": "^22.0.0",
    "@ag-enterprise/grid-column-tool-panel": "^22.0.0",
    "@ag-enterprise/grid-core": "^22.0.0",
    "@ag-enterprise/grid-excel-export": "^22.0.0",
    "@ag-enterprise/grid-filter-tool-panel": "^22.0.0",
    "@ag-enterprise/grid-master-detail": "^22.0.0",
    "@ag-enterprise/grid-menu": "^22.0.0",
    "@ag-enterprise/grid-range-selection": "^22.0.0",
    "@ag-enterprise/grid-rich-select": "^22.0.0",
    "@ag-enterprise/grid-row-grouping": "^22.0.0",
    "@ag-enterprise/grid-server-side-row-model": "^22.0.0",
    "@ag-enterprise/grid-set-filter": "^22.0.0",
    "@ag-enterprise/grid-side-bar": "^22.0.0",
    "@ag-enterprise/grid-status-bar": "^22.0.0",
    "@ag-enterprise/grid-viewport-row-model": "^22.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.0",
    "eslint": "^6.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-ie11": "^1.0.0",
    "glob": "^7.1.4",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-header": "^2.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-tslint": "^8.1.0",
    "gulp-typescript": "^5.0.1",
    "jest": "^24.9.0",
    "merge2": "^1.2.0",
    "postcss-loader": "^3.0.0",
    "protractor": "^5.4.0",
    "rollup": "^1.21.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "start-server-and-test": "^1.10.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.0.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.3",
    "webpack": "^4.39.0",
    "webpack-stream": "5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
